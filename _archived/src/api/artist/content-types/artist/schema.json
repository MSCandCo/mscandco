{
  "kind": "collectionType",
  "collectionName": "artists",
  "info": {
    "singularName": "artist",
    "pluralName": "artists",
    "displayName": "Artist",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "stageName": {
      "type": "string"
    },
    "artistType": {
      "type": "enumeration",
      "enum": [
        "solo",
        "band",
        "producer",
        "composer"
      ]
    },
    "profilePhoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "genre": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "artists"
    },
    "contractStatus": {
      "type": "enumeration",
      "enum": [
        "active",
        "inactive",
        "pending"
      ],
      "default": "pending"
    },
    "dateSigned": {
      "type": "date"
    },
    "socialMedia": {
      "type": "json"
    },
    "manager": {
      "type": "component",
      "component": "manager.manager-info"
    },
    "furtherInfo": {
      "type": "text"
    },
    "creations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::creation.creation",
      "mappedBy": "artist"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "artist"
    }
  }
}
