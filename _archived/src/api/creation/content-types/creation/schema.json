{
  "kind": "collectionType",
  "collectionName": "creations",
  "info": {
    "singularName": "creation",
    "pluralName": "creations",
    "displayName": "Creation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "licenseType": {
      "type": "enumeration",
      "enum": [
        "exclusive",
        "non_exclusive",
        "limited"
      ]
    },
    "licenseDuration": {
      "type": "enumeration",
      "enum": [
        "one_year",
        "two_years",
        "five_years",
        "perpetual"
      ]
    },
    "price": {
      "type": "decimal",
      "min": 0
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "USD",
        "EUR",
        "GBP"
      ],
      "default": "USD"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published",
        "archived"
      ],
      "default": "draft"
    },
    "artist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist",
      "inversedBy": "creations"
    },
    "songs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::song.song",
      "mappedBy": "creation"
    }
  }
} 