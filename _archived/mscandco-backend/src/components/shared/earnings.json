{
  "collectionName": "components_shared_earnings",
  "info": {
    "displayName": "Earnings",
    "description": "Artist earnings and revenue tracking"
  },
  "options": {},
  "attributes": {
    "totalEarnings": {
      "type": "decimal",
      "default": 0
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "GBP",
        "USD",
        "EUR",
        "CAD",
        "AUD"
      ],
      "default": "GBP"
    },
    "lastPayout": {
      "type": "datetime"
    },
    "nextPayout": {
      "type": "datetime"
    },
    "payoutThreshold": {
      "type": "decimal",
      "default": 50
    },
    "pendingAmount": {
      "type": "decimal",
      "default": 0
    },
    "platformEarnings": {
      "type": "json",
      "description": "Earnings breakdown by platform"
    },
    "monthlyGrowth": {
      "type": "decimal",
      "description": "Percentage growth from previous month"
    },
    "topPerformingTracks": {
      "type": "json",
      "description": "Array of top earning tracks"
    },
    "streamingStats": {
      "type": "json",
      "description": "Detailed streaming statistics"
    }
  }
} 