{
  "kind": "collectionType",
  "collectionName": "artists",
  "info": {
    "singularName": "artist",
    "pluralName": "artists",
    "displayName": "Artist",
    "description": "Artist profiles for distribution and publishing"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "stageName": {
      "type": "string",
      "required": true
    },
    "artistType": {
      "type": "enumeration",
      "enum": [
        "solo_artist",
        "band_group",
        "dj",
        "duo",
        "orchestra",
        "ensemble",
        "collective"
      ],
      "required": true
    },
    "profilePhoto": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phoneNumber": {
      "type": "string"
    },
    "genre": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "mappedBy": "artists"
    },
    "contractStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "signed",
        "active",
        "expired",
        "renewal",
        "inactive"
      ],
      "default": "pending"
    },
    "dateSigned": {
      "type": "date"
    },
    "socialMediaHandles": {
      "type": "json"
    },
    "manager": {
      "type": "component",
      "component": "artist.manager-info",
      "required": false
    },
    "furtherInformation": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "artist"
    },
    "songs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::song.song",
      "mappedBy": "singers"
    },
    "lyrics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lyric.lyric",
      "mappedBy": "writers"
    },
    "stems": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::stem.stem",
      "mappedBy": "artists"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "artist"
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "creations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::creation.creation",
      "mappedBy": "artist"
    },
    "performances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist-performance.artist-performance",
      "mappedBy": "artist"
    },
    "journey": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::artist-journey.artist-journey",
      "mappedBy": "artist"
    },
    "collaborations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collaboration.collaboration",
      "mappedBy": "participants"
    },
    "initiatedCollaborations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::collaboration.collaboration",
      "mappedBy": "initiator"
    },
    "aiScore": {
      "type": "decimal",
      "default": 0
    },
    "potentialScore": {
      "type": "decimal",
      "default": 0
    },
    "marketFit": {
      "type": "decimal",
      "default": 0
    },
    "engagementRate": {
      "type": "decimal",
      "default": 0
    },
    "growthRate": {
      "type": "decimal",
      "default": 0
    },
    "careerStage": {
      "type": "enumeration",
      "enum": ["emerging", "developing", "established", "breakthrough", "legendary"],
      "default": "emerging"
    },
    "acquisitionSource": {
      "type": "enumeration",
      "enum": ["organic", "referral", "social_media", "advertising", "partnership", "event", "ai_discovery"],
      "default": "organic"
    },
    "acquisitionDate": {
      "type": "datetime"
    },
    "onboardingCompleted": {
      "type": "boolean",
      "default": false
    },
    "profileCompletion": {
      "type": "decimal",
      "default": 0
    },
    "socialMediaMetrics": {
      "type": "json"
    },
    "audienceDemographics": {
      "type": "json"
    },
    "collaborationPreferences": {
      "type": "json"
    },
    "developmentGoals": {
      "type": "json"
    },
    "achievementBadges": {
      "type": "json"
    },
    "mentorshipStatus": {
      "type": "enumeration",
      "enum": ["none", "seeking", "assigned", "active", "completed"],
      "default": "none"
    },
    "mentor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist"
    },
    "mentees": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist.artist",
      "mappedBy": "mentor"
    },
    "referralCode": {
      "type": "string"
    },
    "referrals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist.artist",
      "mappedBy": "referredBy"
    },
    "referredBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist"
    },
    "recommendations": {
      "type": "json"
    },
    "automatedTriggers": {
      "type": "json"
    },
    "whiteLabelBrand": {
      "type": "string"
    },
    "customBranding": {
      "type": "json"
    },
    "aiInsights": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ai-insights.ai-insights",
      "mappedBy": "artist"
    },
    "communications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::communication.communication",
      "mappedBy": "recipient"
    },
    "codeGroupIntegrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::code-group-integration.code-group-integration",
      "mappedBy": "artist"
    },
    "referralNetworks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::referral-network.referral-network",
      "mappedBy": "referrer"
    },
    "referredBy": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::referral-network.referral-network",
      "mappedBy": "referredArtist"
    }
  }
}
