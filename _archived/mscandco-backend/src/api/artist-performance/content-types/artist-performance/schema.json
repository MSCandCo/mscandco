{
  "kind": "collectionType",
  "collectionName": "artist_performances",
  "info": {
    "singularName": "artist-performance",
    "pluralName": "artist-performances",
    "displayName": "Artist Performance",
    "description": "Real-time artist performance tracking and analytics"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "artist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist",
      "inversedBy": "performances"
    },
    "period": {
      "type": "enumeration",
      "enum": ["daily", "weekly", "monthly", "quarterly", "yearly"]
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "streams": {
      "type": "biginteger",
      "default": 0
    },
    "earnings": {
      "type": "decimal",
      "default": 0
    },
    "engagement": {
      "type": "decimal",
      "default": 0
    },
    "growthRate": {
      "type": "decimal",
      "default": 0
    },
    "platformBreakdown": {
      "type": "json"
    },
    "geographicBreakdown": {
      "type": "json"
    },
    "audienceMetrics": {
      "type": "json"
    },
    "aiScore": {
      "type": "decimal",
      "default": 0
    },
    "potentialScore": {
      "type": "decimal",
      "default": 0
    },
    "marketFit": {
      "type": "decimal",
      "default": 0
    },
    "collaborationOpportunities": {
      "type": "integer",
      "default": 0
    },
    "syncLicensingOpportunities": {
      "type": "integer",
      "default": 0
    },
    "brandPartnershipPotential": {
      "type": "decimal",
      "default": 0
    },
    "careerStage": {
      "type": "enumeration",
      "enum": ["emerging", "developing", "established", "breakthrough", "legendary"]
    },
    "nextMilestone": {
      "type": "string"
    },
    "recommendations": {
      "type": "json"
    }
  }
} 