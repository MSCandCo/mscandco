{
  "kind": "collectionType",
  "collectionName": "ai_insight",
  "info": {
    "singularName": "ai-insight",
    "pluralName": "ai-insights",
    "displayName": "AI Insights",
    "description": "AI/ML generated insights and recommendations"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "artist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::artist.artist",
      "inversedBy": "aiInsights"
    },
    "insightType": {
      "type": "enumeration",
      "enum": ["performance", "collaboration", "market", "career", "audience", "revenue", "growth", "risk"]
    },
    "confidence": {
      "type": "decimal",
      "default": 0
    },
    "priority": {
      "type": "enumeration",
      "enum": ["low", "medium", "high", "critical"],
      "default": "medium"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "data": {
      "type": "json"
    },
    "recommendations": {
      "type": "json"
    },
    "actionItems": {
      "type": "json"
    },
    "predictedImpact": {
      "type": "decimal",
      "default": 0
    },
    "timeframe": {
      "type": "enumeration",
      "enum": ["immediate", "short_term", "medium_term", "long_term"],
      "default": "short_term"
    },
    "status": {
      "type": "enumeration",
      "enum": ["new", "reviewed", "implemented", "dismissed"],
      "default": "new"
    },
    "assignedTo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "tags": {
      "type": "json"
    },
    "relatedInsights": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ai-insight.ai-insight"
    },
    "modelVersion": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "datetime"
    },
    "automatedActions": {
      "type": "json"
    }
  }
} 